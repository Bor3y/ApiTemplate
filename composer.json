{
    "name": "bor3y/apitemplate",
    "description": "generate api code base",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Elsayed",
            "email": "sw.ahmed.elsayed@gmail.com"
        }
    ],
    "homepage": "https://github.com/bor3y/apitemplate",
    "keywords": ["Laravel", "ApiTemplate"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bor3y\\ApiTemplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bor3y\\ApiTemplate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bor3y\\ApiTemplate\\ApiTemplateServiceProvider"
            ],
            "aliases": {
                "ApiTemplate": "Bor3y\\ApiTemplate\\Facades\\ApiTemplate"
            }
        }
    }
}
